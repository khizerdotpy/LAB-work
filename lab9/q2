#include <stdio.h>

void swapIntegers(int a, int b)
{
    a = a + b;
    b = a - b;
    a = a - b;

    printf("\nafter swap.\nnumber 1: %d\nnumber 2: %d", a, b);
}

int main()
{
    int a, b;
    printf("enter number 1: ");
    scanf("%d", &a);
    printf("enter number 2: ");
    scanf("%d", &b);

    swapIntegers(a, b);
    return 0;
}
